<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragmenListRest"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.servicios.ListaTiendas">

    <RelativeLayout
        android:layout_width="401dp"
        android:layout_height="185dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="496dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="151dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="26dp"
            android:layout_marginEnd="19dp">

            <View
                android:id="@+id/viewImgRest1"
                android:layout_width="147dp"
                android:layout_height="match_parent"
                android:background="@drawable/imgmifarma" />

            <TextView
                android:id="@+id/tvInfoRest1"
                android:layout_width="178dp"
                android:layout_height="81dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="19dp"
                android:layout_marginEnd="13dp"
                android:layout_marginBottom="8dp"
                android:layout_toEndOf="@+id/viewImgRest1"
                android:text="@string/infoRest" />

            <TextView
                android:id="@+id/tvTitleRest1"
                android:layout_width="157dp"
                android:layout_height="30dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="11dp"
                android:layout_marginEnd="13dp"
                android:layout_toEndOf="@+id/viewImgRest1"
                android:text="@string/titleRest"
                android:textAppearance="@style/title_rest" />
        </RelativeLayout>
    </RelativeLayout>

    <View
        android:id="@+id/view3"
        android:layout_width="378dp"
        android:layout_height="45dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/rectangle_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="34dp"
        android:layout_height="25dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        android:background="?android:attr/actionModeWebSearchDrawable"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toTopOf="@+id/view3">

    </RelativeLayout>

    <EditText
        android:id="@+id/etBuscar"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:hint="@string/buscar"
        android:inputType="text"
        app:layout_constraintStart_toEndOf="@+id/relativeLayout"
        app:layout_constraintTop_toTopOf="@+id/view3" />

    <RelativeLayout
        android:id="@+id/relativeLayout3"
        android:layout_width="379dp"
        android:layout_height="97dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="148dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/button"
            android:layout_width="104dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="18dp"
            android:layout_marginTop="16dp"
            android:text="pollos y carnes"
            android:textSize="10sp" />

        <Button
            android:id="@+id/button2"
            android:layout_width="104dp"
            android:layout_height="38dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="264dp"
            android:layout_marginTop="20dp"
            android:text="Farmacia"
            android:textSize="10sp" />

        <Button
            android:id="@+id/button3"
            android:layout_width="104dp"
            android:layout_height="38dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="138dp"
            android:layout_marginTop="18dp"
            android:text="Despensa"
            android:textSize="10sp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="401dp"
        android:layout_height="185dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="272dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="151dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="26dp"
            android:layout_marginEnd="19dp">

            <View
                android:id="@+id/viewImgRest"
                android:layout_width="147dp"
                android:layout_height="match_parent"
                android:background="@drawable/imtambo" />

            <TextView
                android:id="@+id/tvInfoRest"
                android:layout_width="178dp"
                android:layout_height="81dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="19dp"
                android:layout_marginEnd="13dp"
                android:layout_marginBottom="8dp"
                android:layout_toEndOf="@+id/viewImgRest"
                android:text="@string/infoRest" />

            <TextView
                android:id="@+id/tvTitleRest"
                android:layout_width="157dp"
                android:layout_height="30dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="11dp"
                android:layout_marginEnd="13dp"
                android:layout_toEndOf="@+id/viewImgRest"
                android:text="@string/titleRest"
                android:textAppearance="@style/title_rest" />
        </RelativeLayout>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>